package statements;

public class S002_IfDemo {
	public static void main(String[] args) {
		
		int a = 100;
		int b = 20;
		int c = 500;
		
		
		if(a>b)
		{
			if(a>c)
			{
				System.out.println("a is greater");
			}
			else
			{
				System.out.println("c is greater");
			}
			
		}
		else
		{
			if(b>c)
			{
			System.out.println("b is greter");
			}
			else
			{
				System.out.println("c is greater");
			}
			
		}
		
		
	}
}
