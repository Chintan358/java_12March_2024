package basicpractical;

public class B002_Primecheck {
	public static void main(String[] args) {
		
		
		int num = 15;
		int flag = 0;
		
		for(int i=2; i<num;i++)
		{
			//2 3
			if(num%i==0)
			{
				flag = 1;
				break;
			}
		}
		
		if(flag==1)
		{
			System.out.println("Numer is not prime");
		}
		else
		{
			System.out.println("Number is prime");
		}
		
	}
}
